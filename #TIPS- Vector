#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;
int main()
{
    vector<int> s;
    s.push_back(3);
    s.push_back(1);
    s.push_back(9);
    for (int i = 0; i < s.size(); i++)
    {
        cout << s[i] << endl;
    }

    sort(s.begin(), s.end());
    for (int i = 0; i < s.size(); i++)
    {
        cout << s[i] << "  ";
    }
}
